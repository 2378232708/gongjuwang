page {
    background-color: #f5faff;
}

.title {
    width: 100%;
    height: 8.8rem;
}

.intro {
    margin-top: 5rem;
    width: 90%;
    margin-left: 5%;
}

.down {
    width: 10px;
    height: 10px;
}

.dateSty {
    display: inline-block;
    color: rgba(0,0,0,.6);
    border: 1px solid #b7b4b4;
    padding: 4px 8px;
    border-radius: 6px;
}

.section__title {
    text-align: center;
    font-size: 1.5rem;
    margin-top: 1.5rem;
    font-weight: 900;
}

.picker {
    display: block;
    text-align: center;
    margin-top: .6rem;
    margin-bottom: .5rem;
}

.cacuBtn {
    background-color: #ff686c;
    margin-bottom: .2rem;
}

.cacuBtn,.resetbtn {
    width: 92%;
    margin-left: 4%;
    color: #fff;
    height: 40px;
    line-height: 0px;
    font-size: 1.1rem;
}

.resetbtn {
    background-color: orange;
    margin-top: .1rem;
}

text {
    line-height: 30px;
    margin: 10px 0;
}

.tit {
    display: block;
}

.timeb {
    border: 1px solid #b7b4b4;
    width: 14%;
    display: inline-block;
    text-align: center;
    vertical-align: bottom;
}

.timeone {
    margin-left: 5%;
}

.table {
    border: 0 solid #a9a9a9;
}

.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 2.3rem;
    align-items: center;
}

.td {
    width: 40%;
    padding-left: 15%;
}

.td,.tdone {
    justify-content: center;
    text-align: left;
}

.tdone {
    width: 50%;
    padding-left: 5%;
}

.bg-w {
    background: snow;
}

.bg-g {
    background: #e6f3f9;
}

.th {
    width: 40%;
    justify-content: center;
    background: #36f;
    color: #fff;
    display: flex;
    height: 3rem;
    align-items: center;
}

.tabletit {
    font-weight: 800;
    font-size: 1.1rem;
}

.beizhu {
    display: inline-block;
    font-size: .7rem;
    line-height: 1rem;
    width: 90%;
    margin-left: 5%;
    color: rgba(0,0,0,.6);
    margin-top: 5px;
}