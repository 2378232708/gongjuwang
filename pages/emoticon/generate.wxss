page {
  width: 100%;
  height: 100%;
  background: url(https://wx3.sinaimg.cn/mw1024/69448882gy1fyjkxufex6j20dw08wjsm.jpg) center top;
  background-size: contain;
  background-color: #242424;
}

.container {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 10px 10px;
  position: relative;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}

.iconfont {
  display: block;
  width: 30px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  font-size: 18px;
  color: #fff;
  border-radius: 50%;
  position: absolute;
}

.board {
  background: url(https://wx3.sinaimg.cn/large/69448882gy1fyjkxufanyj200g00gmxa.jpg);
  position: relative;
}

.zan {
  text-align: center;
  margin-bottom: 20rpx;
}

.zan view {
  height: 80rpx;
  line-height: 80rpx;
 
  color: #fff;
  font-size: 42rpx;
}

canvas {
    width: 100%;
    height: 100%;
}
.btns {
    margin: 20rpx -10rpx;
    display: flex;
}

.btns button:first-child {
    flex-basis: 60%;
}

.btns button:last-child {
    flex-basis: 35%;
    background: #13a113;
    color: #fff;
}