page {
    background: #fff;
}

.container,.head {
    position: relative;
}

.head {
    overflow: hidden;
    height: 282rpx;
}

.head image {
    width: 100%;
    height: 100%;
}

.head .head_body {
    top: 24rpx;
    background: url("https://img.51baoshui.com/miniApp/calculator/money_bg.png") no-repeat top;
    background-size: 588rpx 110rpx;
    position: absolute;
    color: #fff;
    left: 0;
    width: 100%;
    text-align: center;
}

.head .head_body .money {
    font-size: 72rpx;
    line-height: 72rpx;
    font-weight: 700;
    padding-top: 5rpx;
}

.head .head_body {
    color: hsla(0,0%,100%,.7);
    font-size: 20rpx;
    margin-top: 2rpx;
}

.head .increase {
    margin-top: 22rpx;
}

.head .increase text {
    color: #f8e71c;
}

.head .button {
    width: 228rpx;
    height: 52rpx;
    border-radius: 100rpx;
    background: hsla(0,0%,100%,.2);
    border: 1px solid hsla(0,0%,100%,.7);
    font-size: 28rpx;
    text-align: center;
    display: inline-block;
    box-sizing: border-box;
    margin-top: 20rpx;
}

.head .button text {
    color: #fff;
    font-size: 24rpx;
    display: inline-block;
    padding-right: 24rpx;
    background: url(https://img.51baoshui.com/miniApp/calculator/arrow_white.png) no-repeat 100%;
    background-size: 24rpx 24rpx;
    font-weight: 700;
    height: 50rpx;
    line-height: 44rpx;
}

.condition {
    width: 686rpx;
    background: #fff;
    box-shadow: 0 4rpx 20rpx 0 rgba(197,216,242,.64);
    border-radius: 20rpx;
    position: relative;
    margin: -100rpx auto 0;
    box-sizing: border-box;
}

.condition .items {
    display: flex;
    overflow: hidden;
    padding: 32rpx 0;
}

.condition .item {
    flex: 1;
    text-align: center;
    position: relative;
}

.condition .item .arr {
    content: " ";
    display: inline-block;
    width: 18rpx;
    height: 12rpx;
    background: url("https://img.51baoshui.com/miniApp/calculator/arrow.png");
    background-size: 100% 100%;
    position: absolute;
    right: -30rpx;
    top: 50%;
    transform: translateY(-50%) rotate(180deg);
    transition: all .5s ease 0s;
}

.condition .item .txt {
    font-size: 24rpx;
    margin-bottom: 20rpx;
    color: #9e9e9e;
    line-height: 24rpx;
}

.condition .item .num {
    font-size: 32rpx;
    font-weight: 700;
    color: #5879fe;
    line-height: 32rpx;
    display: inline-block;
    position: relative;
}

.condition .item .tax {
    color: #ff7752;
}

.condition .deductDetails {
    width: 620rpx;
    margin: 0 auto;
    overflow: hidden;
    max-height: 0;
    transition: max-height .5s ease;
}

.showDetails .deductDetails {
    max-height: 1000px;
}

.showDetails .item .arr {
    transform: translateY(-50%) rotate(0deg);
}

.condition .deductDetails .deduct {
    overflow: hidden;
    padding-bottom: 32rpx;
    position: relative;
}

.condition .deductDetails .deduct text {
    font-size: 24rpx;
    color: #9e9e9e;
    line-height: 34rpx;
}

.condition .deductDetails .deduct text:first-child {
    float: left;
}

.condition .deductDetails .deduct text:last-child {
    float: right;
    color: #666;
    font-weight: 700;
}

.condition .deductDetails .deduct:first-child {
    padding-top: 28rpx;
}

.condition .deductDetails .deduct:first-child::before {
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    height: 1PX;
    border-bottom: 1px solid #cfcfcf;
    color: #d8d8d8;
    transform-origin: 0 0;
    top: 0;
    transform: scaleY(.5) translateY(-50%);
}

.condition .deductDetails .special {
    clear: both;
    padding-left: 36rpx;
    padding-top: 6rpx;
}

.condition .deductDetails .special view {
    margin-bottom: 16rpx;
    overflow: hidden;
    clear: both;
}

.condition .deductDetails .special view:last-child {
    margin-bottom: 0;
}

.reslutList .tips {
    line-height: 32rpx;
    font-size: 24rpx;
    color: #bebebe;
    padding-top: 16rpx;
}

.detail,.formula {
    padding: 0 32rpx;
    margin-top: 20rpx;
}

.detail .tips {
    margin-top: 32rpx;
    text-align: left;
    font-weight: 500;
    font-size: 24rpx;
    color: #cf9300;
    line-height: 40rpx;
    background: #fff0ce;
    border-radius: 8rpx;
    padding: 12rpx 32rpx 12rpx 160rpx;
    position: relative;
}

.detail .tips text:first-child {
    position: absolute;
    text-align: right;
    width: 144rpx;
    left: 0;
    top: 12rpx;
    font-weight: 700;
}

.detail .list {
    margin-top: 38rpx;
}

.detail .list .item {
    font-size: 24rpx;
    color: #9e9e9e;
    line-height: 40rpx;
    padding: 25rpx 0;
    overflow: hidden;
}

.detail .list .txt {
    width: 48%;
    float: left;
}

.detail .list .num {
    width: 48%;
    float: right;
    color: #1e2254;
    text-align: right;
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
}

.detail .list .tax {
    border-top: 1px solid #f5f5f5;
}

.detail .banner {
    width: 100%;
    height: 100rpx;
    margin-bottom: 20rpx;
    margin-top: 20rpx;
    background: linear-gradient(1turn,#5778fe,#6989fe);
    box-shadow: 0 16rpx 20rpx 0 rgba(91,124,254,.18);
    border-radius: 8rpx;
}

.detail .banner text {
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    color: #fff;
}

.detail .title,.formula .title {
    position: relative;
    height: 44rpx;
    padding-left: 20rpx;
    color: #222557;
    font-weight: 500;
    line-height: 44rpx;
    font-size: 32rpx;
}

.detail .title view,.formula .title view {
    position: absolute;
    left: 0;
    top: 12rpx;
    height: 20rpx;
    width: 4rpx;
    background: #419bf9;
    border-radius: 8rpx;
}

.formula .formula_list view {
    margin-top: 28rpx;
    line-height: 46rpx;
    padding-left: 20rpx;
}

.formula .formula_list view text:first-child {
    color: #212557;
    font-weight: 500;
    font-size: 28rpx;
}

.formula .formula_list view text:last-child {
    color: #1e2254;
    font-weight: 300;
    font-size: 26rpx;
    opacity: .7;
}

.tax_rate_table {
    padding: 0 32rpx;
}

.tax_rate_table .title {
    position: relative;
    height: 44rpx;
    padding: 46rpx 0 46rpx 20rpx;
    color: #222557;
    font-weight: 500;
    line-height: 44rpx;
    font-size: 32rpx;
}

.tax_rate_table .title view {
    position: absolute;
    left: 0;
    top: 50%;
    height: 20rpx;
    width: 4rpx;
    background: #419bf9;
    border-radius: 8rpx;
    transform: translateY(-50%);
}

.tax_rate_table image {
    width: 100%;
}

.tax_rate_table .tips {
    margin-top: 20rpx;
    font-size: 24rpx;
    color: #888;
    line-height: 32rpx;
}

.end {
    padding: 20rpx 0 28rpx;
    color: #888;
    opacity: .37;
    font-size: 24rpx;
}

.banner,.end {
    text-align: center;
}

.banner {
    margin-top: 30rpx;
}

.banner image {
    width: 686rpx;
}

.pushButton {
    margin-top: 60rpx;
    width: 100%;
    padding: 0 48rpx;
    box-sizing: border-box;
}

.pushButton .text {
    height: 52rpx;
    background: #eafffb;
    border-radius: 8rpx;
    position: relative;
    padding: 0 32rpx 0 102rpx;
}

.pushButton .text image {
    display: block;
    position: absolute;
    top: -40rpx;
    left: 0;
    width: 80rpx;
    height: 78rpx;
}

.pushButton .text text {
    display: inline-block;
    width: 100%;
    height: 52rpx;
    line-height: 52rpx;
    font-family: PingFangSC-Medium;
    font-size: 24rpx;
    color: #0dcea7;
    background: url("https://img.51baoshui.com/miniApp/calculator/push-arrow.png") no-repeat 100%;
    background-size: 12rpx 20rpx;
}

.ad {
    margin-top: 10rpx;
}