.bor-t {
    border-top: 1px solid #efeff4;
}

.bor-l {
    border-left: 1px solid #efeff4;
}

.t-c {
    text-align: center;
}

.t-l {
    text-align: left;
}

.flex-box {
    display: flex;
}

.flex {
    -webkit-box-flex: 1;
    flex: 1;
    -webkit-box-ordinal-group: 1;
    order: 1;
}

.blue {
    color: #1eacea;
}

.gray {
    color: #888;
}

.gray-bg {
    background: #efeff4;
    height: 30rpx;
}

.relative {
    position: relative;
}

.hide {
    display: none;
}

.inline-block {
    display: inline-block;
}

.hot-city-mt {
    background: #efeff4;
    color: #888;
    padding: 20rpx 30rpx;
}

.city-list {
    font-size: 28rpx;
    padding: 20rpx 30rpx;
}

.inp {
    height: 90rpx;
    line-height: 90rpx;
    display: inline-block;
    padding: 0 10rpx;
    font-size: 28rpx;
    text-align: right;
    color: #888;
    width: 360rpx;
}

.btn {
    padding: 40rpx 30rpx;
}

.btn button {
    background-color: #1eacea;
    color: #fff;
    line-height: 80rpx;
}

.btn .btn-hover {
    background-color: #008ecc;
    transition: background-color .5s;
}

.dk-mt-text {
    height: 86rpx;
}

.dk-mt-text.on {
    border-bottom: 6rpx solid #1eacea;
}

.dk-ceng {
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 30rpx;
}

.icon-check {
    position: relative;
    top: 10rpx;
    height: 50rpx;
}

.psection-inp {
    color: #bfbfbf;
    text-align: right;
}

.dk-slider {
    padding: 20rpx 30rpx;
}

.drag-pole .wx-slider-track {
    background: #1eacea!important;
}

.icon-right {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 16rpx;
    height: 25rpx;
    top: 2rpx;
    position: relative;
}

.policy {
    color: #6a7fa5;
}

.icon-right.icon-policy {
    top: 32rpx;
}

.auto-text {
    height: 90rpx;
    line-height: 90rpx;
    padding: 10rpx 0;
}

.auto-text .inp {
    width: 100rpx;
    text-align: center;
}

.sd-text {
    line-height: 36rpx;
}

.dk-nian-text {
    position: absolute;
    right: 0;
    top: 40rpx;
}

.btn-service {
    font-size: 26rpx;
    position: fixed;
    right: 20rpx;
    bottom: 50rpx;
    text-align: center;
    background-color: rgba(30,172,234,.8);
    border-radius: 8rpx;
    color: #fff;
    padding: 10rpx 28rpx;
    z-index: 50;
}

.tf-logo {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 212rpx 46rpx;
    width: 212rpx;
    height: 46rpx;
    margin: 100rpx auto 20rpx;
}

.monthly {
    background: #efeff4;
    color: #888;
}

.dk-mt {
    height: 90rpx;
    line-height: 90rpx;
}

.dk-mt-list {
    margin: 0 10rpx;
}

.dk-mt-text {
    display: inline-block;
}

.dk-mt-text.on {
    border-bottom: 4rpx solid #1eacea;
}

.back-btn {
    font-size: 39rpx;
    color: #fff;
    position: fixed;
    bottom: 0;
    display: flex;
    width: 100%;
    justify-content: center;
    background-color: #1eacea;
}